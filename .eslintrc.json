{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "plugin:cypress/recommended",
    "prettier",
    "airbnb-base"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "no-shadow": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "quotes": [2, "double"],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
